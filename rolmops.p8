pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function map_unit(_col,_row)
 local me={}
 me.col=_col
 me.row=_row
 local height0=0
 me.height=0
 me.player=0

 me.setwave=function(_wave)
  me.height=height0+_wave
 end

 return me
end

function dirwave_fun(angle)
 local me={}
 local dx=cos(angle)
 local dy=sin(angle)
 local p=90 -- period
 local a=2  -- amplitude
 local w=4  -- wavelength

 me.eval=function(x,y)
  local d=x*dx+y*dy
  return sin(d/w-clock/p)*a
 end

 return me
end

function map_model()
 local me={}

 me.ncol=8
 me.nrow=8
 me.units={}
 me.functions={}

 for c=1,me.ncol do
  me.units[c]={}
  for r=1,me.nrow do
   me.units[c][r]=map_unit(c,r)
  end
 end

 me.functions[1]=dirwave_fun(0.10)

 me.update=function()
  for c=1,me.ncol do
   for r=1,me.nrow do
    local w=0
    for i=1,#me.functions do
     w+=me.functions[i].eval(c,r)
    end
    me.units[c][r].setwave(w)
   end
  end
 end

 return me
end

function isoline()
 local me={}
 me.units={}

 me.add=function(unit)
  me.units[#me.units+1]=unit
 end

 return me
end

function map_view(_model)
 local me={}
 local model=_model
 local isolines={}
 local nlines=model.ncol+model.nrow-1
 local tilecolors={
  {2,14,15},{1,13,12},
  {0,5,6},{10,9,4}
 }

 for i=1,nlines do
  isolines[i]=isoline()
 end
 for c=1,model.ncol do
  for r=1,model.nrow do
   isolines[c+r-1].add(model.units[c][r])
  end
 end

 me.draw=function()
  cls()
  for i=1,nlines do
   units=isolines[i].units
   for j=1,#units do
    unit=units[j]
    x=(unit.col-unit.row)*8+56
    y=(unit.col+unit.row)*4
      -unit.height

    if (i%2)==0 then
     pal()
    else
     --tmp: quadrant to color
     c=tilecolors[
      1+
      shr(band(unit.col-1,4),2)+
      shr(band(unit.row-1,4),2)*2
     ]
     pal(5,c[1])
     pal(6,c[2])
     pal(7,c[3])
    end
    spr(0,x,y,2,4)

    if unit.player==8 then
     player.draw(x,y)
    elseif unit.player>0 then
     pal()
     pal(7,0)
     spr(62+unit.player*2,x,y-8,2,2)
    end
   end
  end
 end

 return me
end

function player(c,r)
 local me={}
 me.x=0
 me.y=0
 local rot_del=4 -- delay
 local rot_max=20*rot_del
 local rot=0     -- [0..rot_max>
 local rot_dir=0 -- -1,0,1
 local mov_dir=0 -- [0..4]

 me.update=function()
  if rot_dir!=0 then
   -- turning
   rot=(rot+rot_dir+rot_max)%rot_max
   if rot%(5*rot_del)==0 then
    -- finished turn
    rot_dir=0
   end
   return
  end

  if btnp(0) then
   rot_dir=-1
  elseif btnp(1) then
   rot_dir=1
  end
 end --update()

 me.draw=function(x,y)
  local r=flr(rot/rot_del)
  pal()
  if r>9 then
   pal(8,10)
   pal(10,8)
  end
  print(rot,0,0,7)
  spr(96+r%10,x+4,y-9,1,2)
 end --draw()

 return me
end --map_mover

function _draw()
 mapview.draw()
end

function _update()
 clock+=1
 mapmodel.update()
 player.update()
end

mapmodel=map_model()
mapmodel.units[4][4].player=1
mapmodel.units[2][4].player=2
mapmodel.units[6][4].player=4
mapmodel.units[8][4].player=5
mapmodel.units[8][6].player=6
mapmodel.units[6][6].player=7
mapmodel.units[4][6].player=8
mapview=map_view(mapmodel)
player=player()
clock=0
