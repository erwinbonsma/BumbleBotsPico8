pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function map_unit(_col,_row)
 local me={}
 me.col=_col
 me.row=_row
 local height0=0
 me.height=0
 me.player=0

 me.setwave=function(_wave)
  me.height=height0+_wave
 end

 return me
end

function dirwave_fun(angle)
 local me={}
 local dx=cos(angle)
 local dy=sin(angle)
 local p=90 -- period
 local a=2  -- amplitude
 local w=4  -- wavelength

 me.eval=function(x,y)
  local d=x*dx+y*dy
  return sin(d/w-clock/p)*a
 end

 return me
end

function map_model()
 local me={}

 me.ncol=8
 me.nrow=8
 me.units={}
 me.functions={}

 for c=1,me.ncol do
  me.units[c]={}
  for r=1,me.nrow do
   me.units[c][r]=map_unit(c,r)
  end
 end

 me.functions[1]=dirwave_fun(0.10)

 me.update=function()
  for c=1,me.ncol do
   for r=1,me.nrow do
    local w=0
    for i=1,#me.functions do
     w+=me.functions[i].eval(c,r)
    end
    me.units[c][r].setwave(w)
   end
  end
 end

 return me
end

function isoline()
 local me={}
 me.units={}

 me.add=function(unit)
  me.units[#me.units+1]=unit
 end

 return me
end

function map_view(_model)
 local me={}
 local model=_model
 local isolines={}
 local nlines=model.ncol+model.nrow-1

 for i=1,nlines do
  isolines[i]=isoline()
 end
 for c=1,model.ncol do
  for r=1,model.nrow do
   isolines[c+r-1].add(model.units[c][r])
  end
 end

 me.draw=function()
  cls()
  for i=1,nlines do
   units=isolines[i].units
   for j=1,#units do
    unit=units[j]
    x=(unit.col-unit.row)*8+56
    y=(unit.col+unit.row)*5
      -unit.height

    if (i%2)==0 then
     pal()
    else
     pal(5,1)
     pal(6,13)
     pal(7,12)
    end
    spr(0,x,y,2,4)

    if (unit.player==1) then
     pal()
     palt(15,1)
     pal(14,0)
     spr(64,x+1,y-8,2,2)
    end
   end
  end
 end

 return me
end

function _draw()
 mapview.draw()
end

function _update()
 clock+=1
 mapmodel.update()
end

mapmodel=map_model()
mapmodel.units[4][4].player=1
mapview=map_view(mapmodel)
clock=0
